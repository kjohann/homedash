{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [">= 1%", "last 2 versions", "Firefox ESR", "IE >= 9", "Safari >= 8", "iOS >= 8", "Edge >= 12"]
      }
    }], "react", "stage-0"
  ],
  "env": {
    "test": {
      "presets": ["env", "react", "stage-0"]
    }
  },
  "plugins": [
    "transform-class-properties"
  ]
}